@{
}
<div>
    <h1>Your Favorite Person</h1>
    <table>
        <tbody>
            <tr>
                <td>
                    First Name: @Model.FirstName
                </td>
            </tr>
            <tr>
                <td>
                    Last Name: @Model.LastName
                </td>
            </tr>
            <tr>
                <td>
                    Age: @Model.Age
                </td>
            </tr>
            <tr>
                <td>
                    Email Address: @Model.EmailAddress
                </td>
            </tr>
            <tr>
                <td>
                    Description: @Model.Description
                </td>
            </tr>
        </tbody>
    </table>
</div>